{
  "name": "blondie",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3000",
    "dev:watch": "nodemon --watch . --ext js,jsx,ts,tsx,json,css --exec 'npm run dev'",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "pdf:bdc": "node ./scripts/export-bdc-pdf.mjs",
    "pdf:convention": "node ./scripts/export-convention-pdf.mjs",
    "pdf:lettre": "node ./scripts/export-lettre-pdf.mjs",
    "pdf:all": "npm run pdf:bdc && npm run pdf:convention && npm run pdf:lettre"
  },
  "dependencies": {
    "@sanity/client": "^6.20.1",
    "@sanity/image-url": "^1.0.2",
    "autoprefixer": "^10.0.0",
    "next": "13.4.12",
    "postcss": "^8.4.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sanity": "^3.39.0",
    "tailwindcss": "^3.2.0"
  },
  "devDependencies": {
    "dotenv": "^17.2.1",
    "nodemon": "^3.1.10",
    "puppeteer": "^22.13.1",
    "sucrase": "^3.34.0"
  }
}
